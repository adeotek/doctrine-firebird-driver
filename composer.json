{
    "name": "kafoso/doctrine-firebird-driver",
    "type": "library",
    "description": "FirebirdSQL driver for the Doctrine DBAL",
    "authors": [
        {
            "name": "Kasper SÃ¸fren",
            "email": "Kasper.Sofren@ist.com"
        },
        {
            "name": "Uffe Petersen",
            "email": "Uffe.Petersen@ist.com"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IST\\DoctrineFirebirdDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IST\\DoctrineFirebirdDriver\\Test\\Resource\\": "tests/resources/Test/Resource",
            "IST\\DoctrineFirebirdDriver\\Test\\Integration\\": "tests/tests/Test/Integration",
            "IST\\DoctrineFirebirdDriver\\Test\\Unit\\": "tests/tests/Test/Unit"
        }
    },
    "require": {
        "doctrine/dbal": "^2.6",
        "php": "^7.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^5.7"
    }
}
